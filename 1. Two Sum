//bruteforce
class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int n = nums.size();
        for(int i = 0 ; i < n-1 ; i++)

            for(int j = i +1 ; j < n ; j++ )

                if(nums[i] + nums[j] == target)
                    return {i , j};

            return {};

    }
};
/*
Runtime: 409 ms, faster than 28.20% of C++ online submissions for Two Sum.
Memory Usage: 10.3 MB, less than 66.65% of C++ online submissions for Two Sum.
*/
